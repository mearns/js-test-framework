{
  "name": "js-test-framework",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "keywords": [
    "testing",
    "test",
    "unit tests",
    "unit testing"
  ],
  "scripts": {
    "clean": "rimraf coverage/ .nyc_output/ dist/",
    "check:lint:readme:markdown": "remark --no-stdout README.md",
    "check:lint:readme": "npm-run-all --parallel check:lint:readme:*",
    "check:lint:js:src": "standard src/**/*.js | snazzy",
    "check:lint:js:test": "standard test/**/*.js | snazzy",
    "check:lint:js:npmpackagejsonlint-config": "standard ./npmpackagejsonlint.config.js | snazzy",
    "check:lint:js": "npm-run-all --parallel check:lint:js:*",
    "check:lint:package-json": "npmPkgJsonLint .",
    "check:lint": "npm-run-all --parallel check:lint:*",
    "check": "npm-run-all --parallel check:*",
    "test": "nyc mocha ./test/src/**/*.spec.js",
    "verify": "npm-run-all check test",
    "compile-prod:app": "babel src/ --out-dir dist/src/ --copy-files",
    "compile-dev:app": "babel src/ --out-dir dist/src/ --plugins node-source-map-support --source-maps inline --copy-files",
    "compile-dev:test": "babel test/ --out-dir dist/test/ --plugins node-source-map-support --source-maps inline --copy-files",
    "compile-prod": "npm-run-all compile-prod:* compile:*",
    "compile-dev": "npm-run-all compile-dev:* compile:*",
    "compile": "if-env NODE_ENV=production && npm run compile-prod || npm run compile-dev",
    "build": "npm-run-all clean verify compile",
    "start": "node dist/src/index.js",
    "test-query": "node dist/src/jira-query.js"
  },
  "author": "Brian Mearns",
  "license": "MIT",
  "engines": {
    "node": ">=5.0.0 <9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mearns/js-test-framework"
  },
  "bugs": {
    "url": "https://github.com/mearns/js-test-framework/issues",
    "email": "mearns.b@gmail.com"
  },
  "homepage": "https://github.com/mearns/js-test-framework",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-assign",
      "transform-object-rest-spread",
      "transform-class-properties",
      [
        "babel-plugin-transform-builtin-extend",
        {
          "globals": [
            "Error"
          ]
        }
      ]
    ]
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ],
    "require": [
      "babel-core/register"
    ],
    "include": [
      "src/**"
    ],
    "all": true,
    "check-coverage": true,
    "statements": 90,
    "branches": 90,
    "functions": 90,
    "lines": 90
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-plugin-node-source-map-support": "0.0.1",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "copyfiles": "2.0.0",
    "eslint": "^4.0.0",
    "if-env": "1.0.4",
    "mocha": "5.2.0",
    "npm-package-json-lint": "3.1.0",
    "npm-run-all": "4.1.3",
    "nyc": "12.0.2",
    "remark-cli": "5.0.0",
    "rimraf": "2.6.2",
    "snazzy": "7.1.1",
    "source-map-support": "0.5.6",
    "standard": "11.0.1"
  },
  "dependencies": {
    "bluebird": "3.5.1"
  }
}
